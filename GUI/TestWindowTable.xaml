<Window x:Class="MetaMorpheusGUI.TestWindowTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetaMorpheusGUI"
        Title="asdf" Height="750" Width="750" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid Name="DG1" ItemsSource="{Binding}" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="Parameter Name"  Binding="{Binding FirstName}" MinWidth ="150" CanUserResize="True"/>
                <DataGridTextColumn Header="Value"  Binding="{Binding FirstName}" CanUserResize="True"/>
                <DataGridTextColumn Header="Unset"  Binding="{Binding FirstName}" CanUserResize="True"/>
                <DataGridTextColumn Header="Different In Multiple Files"  Binding="{Binding FirstName}" CanUserResize="True"/>
            </DataGrid.Columns>
        </DataGrid>

        <ScrollViewer VerticalScrollBarVisibility="Auto">
        </ScrollViewer>

    </Grid>
</Window>
